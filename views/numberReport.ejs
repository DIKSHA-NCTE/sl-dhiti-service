<div class="container" style="padding:20px;margin:20px;">
    <% for(var i=0; i < instaRes.length; i++) { %>
    <% if(instaRes[i].responseType == 'number'){ %>
    <h3><%=  instaRes[i].question %></h3>
    <% for(var j=0; j < instaRes[i].answers.length; j++) { %>
    <div class="row">
      <div class="col-sm-12">
        <div class="col-sm-12" style="border: 1px solid #eae8e8;padding: 10px;">
          <div class="col-sm-12" style="font-size: 18px;">
            <%= instaRes[i].answers[j] %>
          </div>
        </div>
      </div>
    </div> <% if(j == instaRes[i].answers.length - 1){ %>
    <p style="page-break-before: always;">&nbsp;</p>
    <% } %>
    <% } %>
    <% } %>
    <% } %>
  
  </div>