<div class="container" style="padding:20px;margin:20px;">
    <% if(instaResponse.responseType == 'date'){ %>
      <h3><%=  instaResponse.question %></h3>
      <% for(var j=0; j < instaResponse.answers.length; j++) { %>
            <div class="row">
              <div class="col-sm-12">
                <div class="col-sm-12" style="border: 1px solid #eae8e8;padding: 10px;">
                  <div class="col-sm-12" style="font-size: 18px;">
                    <%= instaResponse.answers[j] %>
                  </div>
                </div>
              </div>
            </div> 
          <% if(j == instaResponse.answers.length - 1){ %>
              <p style="page-break-before: always;">&nbsp;</p>
          <% } %>
      <% } %>
    <% } %>
    
 </div>